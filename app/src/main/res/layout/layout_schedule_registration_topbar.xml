<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/schedule_registration_topbar_back_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/task_on_date_back"
        app:layout_constraintTop_toTopOf="@id/schedule_registration_topbar_title_tv"
        app:layout_constraintBottom_toBottomOf="@id/schedule_registration_topbar_title_tv"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/schedule_registration_topbar_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="일정 등록하기"
        android:fontFamily="@font/poppins_bold"
        android:textSize="18sp"
        android:textColor="#1E1E1E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
        android:id="@+id/schedule_registration_topbar_cancel_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/topbar_cancel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/schedule_registration_topbar_title_tv"
        app:layout_constraintBottom_toBottomOf="@id/schedule_registration_topbar_title_tv"/>
</androidx.constraintlayout.widget.ConstraintLayout>